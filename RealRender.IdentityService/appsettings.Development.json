{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext}{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "identitydb": "Server=realrender.dbservice;Port=5432;Database=identity;User Id=postgres;Password=postgres;",
    "identityserverdb": "Server=realrender.dbservice;Port=5432;Database=identityserver;User Id=postgres;Password=postgres;"
  }
}
